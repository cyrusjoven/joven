style.css
.name{
  width:100%;
  height:10a%;
  background:#41b3a3;
  color:beige;
  font-size: 25px;
  font-style:monospace;
  text-align: center;
  border: 1px solid ghostwhite;
}
.menu{
    height: 80%;
    width: 25%;
    background:lightgrey;
    color: lightskyblue;
    font-size: 25px;
    font-family: monospace;
    text-align: center;
    border: 1px solid ghostwhite;
    line-height: 50px;
}
.body{
float: right;
height: 80%;
     width: 75%;
     background-color:#283747;
     border: 1px solid ghostwhite;
}
.body h2{
     text-align: center;
     color:whitesmoke;
     font-size: 25px;
}
.body h3{
     text-align: center;
     color:whitesmoke ;
     font-size: 22px;
}
.body p{
     font-size: 18px;
     text-align: center;
     color:#66fcf1;
}
.footer{
    height: 10%;
    width: 100%;
    color: black;
    background:#41b3a3;
    text-align:center;
    font-size:22px;
    font-style: monospace;
    border: 1px solid ghostwhite;
}

login.css
.body{
  margin: 0;
  padding: 0;
  background:#5D6D7E ;
  background-size:100%;
  background-position: center;
  font-family: monospace;

}
.loginbox{
  width:370px;
  height: 480px;
  background:#424242  ;
  color:#17202A  ;
  top:50%;
  left:50%;
  position:absolute ;
  transform: translate(-50%,-50%);
  box-sizing: border-box;
  padding: 50px 50px;
 
}
.avatar{
  width: 100px;
  height: 100px;
  border-radius: 50%;
  position: absolute;
  top: -50px;
  left: calc(50% - 50px);
}
.h1{
  margin:0;
  padding:0 0 20px;
  text-align: center;
  font-size: 22px;
  color: white;
}
.loginbox p{

  font-weight: bold;
  color: white;
}
.loginbox input{
  width: 100%;
  margin-bottom:20px ;
}
.loginbox input[type="text"], input[type="password"]{
  border: none;
  border-bottom:1px solid white ;
  background: transparent;
  outline: none;
  height: 40px;
  color: whitesmoke;
  font-size: 14px;
}
.loginbox input[type="submit"]{
  border: 1px solid whitesmoke;
  outline:none ;
  height: 40px;
  background:#E0F7FA;
  color: black;
  font-size: 18px;
  width: 100%;
  border-radius: 25px;
}
.loginbox input[type="submit"]:hover{
  cursor: pointer;
  background: #006064;
  color: white;

}
.loginbox a{
  text-decoration: none;
  font-size: 15px;
  line-height: 20px;
  color: #FDFEFE ;
}
.loginbox a:hover{
  background-color: #ddd;
  color: black;
} 
